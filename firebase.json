{
	"hosting": {
		"headers": [ {
			"source": "**/*.*",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate, max-age=0"
				}
			]
		}],
		"public": "deploy/public",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		]
	}
}